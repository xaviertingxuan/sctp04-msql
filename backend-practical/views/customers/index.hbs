{{#extends "base"}}
{{#block"content"}}


<h1>Customers</h1>
<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Rating</th>
        <th>Company</th>
    </tr>
    {{#each customers}}
    <tr>
        <td>{{first_name}}</td>
        <td>{{last_name}}</td>
        <td>{{rating}}</td>
        <td>{{name}}</td>
    </tr>
    {{/each}}
</table>
{{/block}}
{{/extends}}
